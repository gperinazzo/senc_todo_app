[Unit]
Description=uWSGI Emperor
After=syslog.target

[Service]
ExecStart=/root/senc_todo_app/env/bin/uwsgi --ini /root/senc_todo_app/uwsgi.ini
RuntimeDirectory=/root/senc_todo_app
Restart=always
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target